            <RowDefinition Height="50" />
            <RowDefinition Height="50" />
            <RowDefinition Height="50" />
            <RowDefinition Height="50" />
            <RowDefinition Height="50" />
        </Grid.RowDefinitions>
        <Button x:Name="Row1Btn" Margin="10,0,2,2" Grid.Row="0" Grid.Column="0" />
        <Label x:Name="Row1Lbl" Text="Description of Button 1 here..." Margin="2,0,10,2" Grid.Row="0" Grid.Column="1"/>
